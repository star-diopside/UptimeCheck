<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyName>UptimeCheck</AssemblyName>
    <RootNamespace>UptimeCheck</RootNamespace>
    <ApplicationManifest>..\UptimeCheck\app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\UptimeCheck\**\*.cs" />
    <EmbeddedResource Include="..\UptimeCheck\**\*.resx" />
  </ItemGroup>

</Project>
